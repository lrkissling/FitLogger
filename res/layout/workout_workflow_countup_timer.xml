<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
	<TextView 
	  	android:id="@+id/tvWorkoutWorkflowCountdownTimerExerciseName"
	  	android:layout_width="match_parent"
	  	android:layout_height="wrap_content"
		android:gravity="center_vertical|center_horizontal"
		android:padding="10dip"
	  	android:text="Exercise Name"
	/>
	
	<Chronometer 
		android:id="@+id/chrWorkoutWorkflowCountdownTimer"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
	/>
	
	<TextView 
	    android:id="@+id/tvWorkoutWorkflowCountdownTimerTimer"
	    android:layout_width="match_parent"
	    android:layout_height="wrap_content"
	    android:gravity="center_vertical|center_horizontal"
	    android:padding="10dip"
	    android:text="Timer"
	/>
	
	<Button 
		android:id="@+id/btnWorkoutWorkflowCountdownTimerStartPause"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:paddingLeft="10dip"
		android:paddingRight="10dip"
		android:text="Start"
	/>
       
    <TableLayout 	
	    android:id="@+id/tlWorkoutWorkflowSetHistoryNoteRow"
       	android:layout_width="fill_parent"
       	android:layout_height="wrap_content"
       	android:stretchColumns="*"
    >
	</TableLayout>

	

	
	<TableLayout 	
	    android:id="@+id/tlWorkoutWorkflowSetResults"
       	android:layout_width="fill_parent"
       	android:layout_height="wrap_content"
       	android:stretchColumns="*"
    >
	</TableLayout>
</LinearLayout>